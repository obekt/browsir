# BrowSir API Configuration
# Copy this file to .env and set your actual values

# OpenAI API Key (Required)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key-here

# OpenAI Model (Optional, default: gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# Browser Timeout in seconds (Optional, default: 30)
SELENIUM_TIMEOUT=30

# Maximum popup detection retries (Optional, default: 3)
MAX_POPUP_RETRIES=3

# Run browser in headless mode (Optional, default: true)
CHROME_HEADLESS=true

# Logging level (Optional, default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO